<div class="row">
  <div class="col-xs-12 col-md-4">
    <%- include('../partials/navigation') %>

  </div>
  <div class="col-xs-12 col-md-8">
    <h1><%- pageData.title %></h1>
    <p>
      <%- pageData.description %>
    </p>
    <hr>
    <% if (pageData.items) { 
      for(i=0; i<pageData.items.length; i++) {
        var currentItem = pageData.items[i];
        %>
