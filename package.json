{
  "name": "qubejs-core",
  "version": "2.0.0",
  "scripts": {
    "client:test": "node scripts/test.js --no-watch",
    "client:test:watch": "node scripts/test.js",
    "client:test:coverage": "node scripts/test.js --no-watch --coverage",
    "server:test": "mocha --file ./tests/setup.js  --exit --recursive --config ./mocha-server.config.js",
    "server:test:coverage": "nyc --reporter=html --reporter=lcov --reporter=text mocha --file ./tests/setup.js  --exit --recursive --config ./mocha-server.config.js",
    "server:test:junit": "nyc --reporter=html --reporter=lcov --reporter=text mocha  --reporter mocha-junit-reporter --reporter-options mochaFile=coverage/server/junit.xml --file ./tests/setup.js  --exit --recursive --config ./mocha-server.config.js",
    "server:test:watch": "mocha  -w --recursive --config ./mocha-server.config.js",
    "test": "npm run server:test && npm run client:test",
    "build:apps:rollup": "rollup -c rollup.cms.config.js --environment INCLUDE_DEPS,BUILD:production",
    "build:apps": "webpack --config webpack.build.core.config.js && webpack --config webpack.build.apps.config.js",
    "test:coverage": "npm run server:test:junit && npm run client:test:coverage",
    "start": "start-storybook -p 6006",
    "clean:dist": "rm -rf dist && npm run dist",
    "dist": "npm run build:apps && webpack && webpack --config webpack.web.config.js && node scripts/dist.js",
    "dist:push": "npm run dist && cd dist && git add . && git commit -m 'v2.x.x-update' && git push",
    "dist:target": "npm version patch && rm -rf dist && webpack && webpack --config webpack.web.config.js && node scripts/dist.js && cd dist && npm pack && cd .. && node scripts/dist-post.js",
    "build-storybook": "build-storybook"
  },
  "license": "MIT",
  "nyc": {
    "all": true,
    "include": [
      "server/**/**/*.js",
      "cms/server/**/**/*.js"
    ],
    "exclude": [
      "cms/server/**/*spec.js",
      "server/**/*spec.js",
      "server/test/**/*.js",
      "server/mock/**/*.js"
    ],
    "report-dir": "./coverage/server"
  },
